<p id="notice"><%= notice %></p>

<h1>External Referee Submissions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @external_referee_submissions.each do |external_referee_submission| %>
      <tr>
        <td><%= link_to 'Show', external_referee_submission %></td>
        <td><%= link_to 'Edit', edit_external_referee_submission_path(external_referee_submission) %></td>
        <td><%= link_to 'Destroy', external_referee_submission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

